            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/recycleview_txt_item_number"
            android:clickable="false"
            android:gravity="center_vertical"
            android:text="@string/text_more_number"
            android:textColor="@color/txt_txt_answer_color"
            android:textSize="@dimen/candidateFrontSize" />

        <Button
            android:id="@+id/answer_01_button_01"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@null"
